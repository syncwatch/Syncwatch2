{
  "name": "syncwatch-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve-dist": "browser-sync dist/syncwatch-frontend --no-open --no-ui --single --files=\"dist/**/*\" --port 4200",
    "start": "concurrently \"npm:watch\" \"npm:serve-dist\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "watch-local": "ng build --watch --configuration local",
    "watch-prod": "ng build --watch --configuration production",
    "local": "concurrently \"npm:watch-local\" \"npm:serve-dist\"",
    "serve-local": "ng serve --configuration local",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/localize": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "~13.3.0",
    "daisyui": "^2.17.0",
    "dexie": "^3.2.2",
    "esbuild": "^0.14.38",
    "ngx-device-detector": "^3.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "video.js": "^7.18.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.4",
    "@angular/cli": "~13.3.4",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/video.js": "^7.3.38",
    "autoprefixer": "^10.4.7",
    "browser-sync": "^2.27.9",
    "concurrently": "^7.1.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.2"
  }
}
